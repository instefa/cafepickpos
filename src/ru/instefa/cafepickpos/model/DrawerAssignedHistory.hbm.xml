<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="ru.instefa.cafepickpos.model">
	<class lazy="false" name="ru.instefa.cafepickpos.model.DrawerAssignedHistory" table="DRAWER_ASSIGNED_HISTORY">
		<id column="ID" name="id" type="java.lang.Integer">
			<generator class="hilo">
				<param name="table">id_table</param>
				<param name="column">next_value</param>
				<param name="max_lo">1</param>
			</generator>
		</id>

		<property column="TIME" name="time" type="java.sql.Timestamp" />
		<property column="OPERATION" length="60" name="operation" type="java.lang.String" />

		<!-- Associations -->

		<many-to-one class="ru.instefa.cafepickpos.model.User" lazy="false" name="user">
			<column name="A_USER" />
		</many-to-one>
	</class>
</hibernate-mapping>

<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="ru.instefa.cafepickpos.model">
	<class lazy="false" name="ru.instefa.cafepickpos.model.DataUpdateInfo" table="DATA_UPDATE_INFO">

		<id column="ID" name="id" type="java.lang.Integer">
			<generator class="hilo">
				<param name="table">id_table</param>
				<param name="column">next_value</param>
				<param name="max_lo">1</param>
			</generator>
		</id>

		<property column="LAST_UPDATE_TIME" name="lastUpdateTime" type="java.sql.Timestamp" />
	</class>
</hibernate-mapping>

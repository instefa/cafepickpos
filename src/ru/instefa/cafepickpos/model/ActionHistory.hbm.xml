<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="ru.instefa.cafepickpos.model">
	<class lazy="false" name="ru.instefa.cafepickpos.model.ActionHistory" table="ACTION_HISTORY">
		<id column="ID" name="id" type="java.lang.Integer">
			<generator class="hilo">
				<param name="table">id_table</param>
				<param name="column">next_value</param>
				<param name="max_lo">1</param>
			</generator>
		</id>
		<property column="ACTION_TIME" name="actionTime" type="java.util.Date" />
		<property column="ACTION_NAME" name="actionName" type="string" />
		<property column="DESCRIPTION" name="description" type="string" />
		<!-- Associations -->
		<many-to-one class="User" column="USER_ID" name="performer">

		</many-to-one>
	</class>
</hibernate-mapping>

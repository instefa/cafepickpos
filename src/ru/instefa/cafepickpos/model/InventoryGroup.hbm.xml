<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="ru.instefa.cafepickpos.model">
	<class lazy="false" name="ru.instefa.cafepickpos.model.InventoryGroup" table="INVENTORY_GROUP">
		<id column="ID" name="id" type="java.lang.Integer">
			<generator class="hilo">
				<param name="table">id_table</param>
				<param name="column">next_value</param>
				<param name="max_lo">1</param>
			</generator>
		</id>


		<property column="NAME" length="60" name="name" not-null="true" type="java.lang.String" />
		<property column="VISIBLE" name="visible" type="java.lang.Boolean" />

	</class>
</hibernate-mapping>